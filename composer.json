{
    "name": "samandar/laravel-elevenlabs",
    "description": "Laravel package for ElevenLabs Text-to-Speech API integration with full API support",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "elevenlabs",
        "text-to-speech",
        "tts",
        "ai",
        "voice",
        "speech-to-text",
        "voice-cloning",
        "audio-ai"
    ],
    "authors": [
        {
            "name": "Abdullaev Samandar",
            "email": "xkas2001@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Samandar\\LaravelElevenLabs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Samandar\\LaravelElevenLabs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Samandar\\LaravelElevenLabs\\ElevenLabsServiceProvider"
            ],
            "aliases": {
                "ElevenLabs": "Samandar\\LaravelElevenLabs\\Facades\\ElevenLabs"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
